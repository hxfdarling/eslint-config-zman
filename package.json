{
  "author": "zman",
  "bugs": {
    "url": "https://github.com/hxfdarling/eslint-config-zman/issues"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0"
  },
  "description": "",
  "homepage": "https://github.com/hxfdarling/eslint-config-zman#readme",
  "license": "ISC",
  "main": "index.js",
  "name": "eslint-config-zman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxfdarling/eslint-config-zman.git"
  },
  "scripts": {
    "lint": "eslint --fix index.js",
    "postinstall": "node ./scripts/post-install.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "version": "1.0.0",
  "devDependencies": {
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^8.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  }
}
