{
  "author": "zman",
  "bugs": {
    "url": "https://github.com/hxfdarling/eslint-config-zman/issues"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "description": "",
  "homepage": "https://github.com/hxfdarling/eslint-config-zman#readme",
  "license": "ISC",
  "main": "index.js",
  "name": "eslint-config-zman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxfdarling/eslint-config-zman.git"
  },
  "scripts": {
    "lint": "eslint --fix index.js",
    "postinstall": "node ./scripts/post-install.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.2.1",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.3",
    "prettier": "^1.14.3",
    "standard-version": "^4.4.0"
  }
}
